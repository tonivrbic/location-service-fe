{
  "hosting": {
    "public": "dist",
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }],
    "headers": [{
      "source": "**/*.@(jpg|jpeg|gif|png|js|css|json)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=8640000"
      }]
    }]
  }
}
